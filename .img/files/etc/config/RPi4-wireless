
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'platform/soc/fe300000.mmcnr/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option htmode 'VHT20'
	option channel '36'
	option country 'CN'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'RPi4-WiFi'
	option key 'password'
	option encryption 'psk2'

config wifi-iface 'wifinet1'
	option ssid 'Danke39421_5G'
	option device 'radio0'
	option mode 'sta'
	option key 'Password'
	option network 'wwan3'
	option encryption 'psk2'
	option bssid 'D0:EE:07:67:D9:80'
	option disabled '1'
